Caso: Gestionar todos los trámites académicos de los alumnos.


Class Alumno{

    public int CIAlumno
    public string NombreAlumno
    public string ApellidoAlumno
    public string DireccionAlumno
    private string Carrera

    public setCarrera(Carrera){
        CarreraElegida = Carrera
        return CarreraElegida
    }

    public getCarrera(){
        return Carrera
    }

    private ObtenerHistorialCarrera(NombreAlumno)
    public InscribirseCarrera(NombreAlumno)
    public ModificarseCarrera(NombreAlumno, Carrera)
    public BorrarseCarrera(NombreAlumno,Carrera)

}

Class Tramite{

    public int CodigoTramite
    public int TipoTramite
    public date FechaTramite
    public bool EstadoTramite
    private string Alumno
    private string Carrera

 
    public setEstadoTramite(Fecha, TipoTramite, Carrera, Alumno){
        EstadoTramite = True
        return CodigoTramite
    }

    public getEstadoTramite(CodigoTramite){
        return EstadoTramite
    }


}

